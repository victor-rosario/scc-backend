{
	"compilerOptions": {
		"lib": ["es5", "es6", "dom", "es2017"],
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"incremental": true,
		"outDir": "build",
		"rootDir": "src",
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"inlineSourceMap": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"noUnusedLocals": true,
		"useUnknownInCatchVariables": false,
		"noUnusedParameters": true,
		"noImplicitReturns": false,
		"noFallthroughCasesInSwitch": true,
		"traceResolution": false,
		"listEmittedFiles": false,
		"listFiles": false,
		"pretty": true,
		"types": ["node"],
		"typeRoots": ["node_modules/@types"],
		"baseUrl": "src",
		"paths": {
			"@config": ["config"],
			"@config/*": ["config/*"],
			"@controllers": ["controllers"],
			"@controllers/*": ["controllers/*"],
			"@constants": ["constants"],
			"@constants/*": ["constants/*"],
			"@database": ["database"],
			"@database/*": ["database/*"],
			"@dto": ["dto"],
			"@dto/*": ["dto/*"],
			"@interfaces": ["interfaces"],
			"@interfaces/*": ["interfaces/*"],
			"@providers": ["providers"],
			"@providers/*": ["providers/*"],
			"@libs": ["libs"],
			"@libs/*": ["libs/*"],
			"@middlewares": ["middlewares"],
			"@middlewares/*": ["middlewares/*"],
			"@prototypes": ["prototypes"],
			"@prototypes/*": ["prototypes/*"],
			"@routers": ["routers"],
			"@routers/*": ["routers/*"],
			"@services": ["services"],
			"@services/*": ["services/*"],
			"@utils": ["utils"],
			"@utils/*": ["utils/*"],
			"@memory/*": ["memory/*"],
			"@events/*": ["events/*"]
		}
	},
	"ts-node": {
		"files": true,
		"require": ["tsconfig-paths/register"]
	},
	"include": ["src", "global.d.ts", "index.ts"],
	"exclude": ["node_modules/**"],
	"files": ["./src/types/index.d.ts", "./src/types/express.d.ts"],
	"compileOnSave": false
}
